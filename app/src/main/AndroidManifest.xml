<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.changelaunchericon">

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">

        <activity android:name="com.example.changelaunchericon.MainActivity">
        </activity>

        <activity-alias
                android:targetActivity="com.example.changelaunchericon.MainActivity"
                android:name="com.example.changelaunchericon.Default"
                android:label="IconApp"
                android:icon="@mipmap/ic_launcher_round"
                android:enabled="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity-alias>

        <activity-alias
                android:targetActivity="com.example.changelaunchericon.MainActivity"
                android:name="com.example.changelaunchericon.Launcher1"
                android:label="IconApp"
                android:icon="@mipmap/ic_launcher_1_round"
                android:enabled="false">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity-alias>

        <activity-alias
                android:targetActivity="com.example.changelaunchericon.MainActivity"
                android:name="com.example.changelaunchericon.Launcher2"
                android:label="IconApp"
                android:icon="@mipmap/ic_launcher_2_round"
                android:enabled="false">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity-alias>

    </application>

</manifest>